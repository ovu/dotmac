{
    "window.commandCenter": 1,
    "files.watcherExclude": {
        "**/.bloop": true,
        "**/.metals": true
    },
    "workbench.colorTheme": "onedark.nvim",
    "workbench.iconTheme": "material-icon-theme",
    "vim.insertModeKeyBindings": [],
    "vim.insertModeKeyBindingsNonRecursive": [
        {
            "before": ["j", "k"],
            "after": ["<Esc>"]
        }
    ],
    "vim.handleKeys": {
        "<Esc>": true
    }
    
}